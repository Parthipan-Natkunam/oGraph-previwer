{
  "name": "OGraph Previewer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A light weight chrome extension to preview the open graph tags of the current page",
  "homepage_url": "http://levitatecode.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["http://*/*", "https://*/*"],
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "oGraph Previewer",
    "default_popup": "src/browser_action/browser_action.html"
  }
}
